#ifndef CNN_HLS_RISCV_H
#define CNN_HLS_RISCV_H

#include "macro_CNN_RISCV.h"

/**
 * @brief Function used to make the CNN in HLS version
 * 
 * @param imageResizeOut 		: The pointer to the array of the resize image
 * @param CNNArray1 			: The pointer to work array 1 of the CNN
 * @param CNNArray2 			: The pointer to work array 2 of the CNN
 * @return int 					: 0 if no problem was found / -1 in the other case
 */
int CNN_RISCV(IMG_RESIZE_OUT_TYPE imageResizeOut[], CNN_GLOBAL_TYPE CNNArray1[], CNN_GLOBAL_TYPE CNNArray2[]);

#endif /* CNN_HLS_RISCV_H */
